{
   "action": {
      "default_icon": "img/logo16.png",
      "default_title": "Click to Run CSS Scanner"
   },
   "background": {
      "service_worker": "js/background.js"
   },
   "content_scripts": [ {
      "all_frames": true,
      "css": [ "css/cssviewer.css" ],
      "js": [ "js/cssviewer.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_start"
   } ],
   "description": "The simplest css property viewer.",
   "host_permissions": [ "http://*/*", "https://*/*", "file://*/*", "file://*/*" ],
   "icons": {
      "16": "img/logo16.png",
      "24": "img/logo24.png",
      "32": "img/logo32.png",
      "48": "img/logo48.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "CSS Scanner",
   "permissions": [ "tabs", "contextMenus", "storage", "scripting" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "3.0",
   "web_accessible_resources": [ {
      "matches": [ "\u003Call_urls>" ],
      "resources": [ "img/trash.svg", "img/copy.svg", "img/code.svg", "img/size.svg", "img/font.svg", "img/close.svg", "img/options.svg", "img/pause.svg", "img/play.svg", "img/arrow_down.svg", "img/lightning.svg", "img/logo128.png", "img/bmc-button.svg" ]
   } ]
}
